{
  "name": "reactpro-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start:dev:server": "node json-server/index.cjs",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "babel": "rm -rf i18n-dump && babel src --extensions \".ts,.tsx\" --out-dir i18n-dump && rm -rf i18n-dump",
    "lints": "eslint . --ext .ts,.tsx",
    "lintf": "eslint . --ext .ts,.tsx --fix",
    "style": "npx stylelint \"**/*.scss\"",
    "stylef": "npx stylelint \"**/*.scss\" --fix",
    "test": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "axios": "^1.9.0",
    "i18next": "^25.1.1",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
    "json-server": "^0.17.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.5.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@chromatic-com/storybook": "^3.2.6",
    "@eslint/js": "^9.26.0",
    "@eslint/json": "^0.12.0",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/experimental-addon-test": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.14.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.1.3",
    "@vitest/coverage-v8": "^3.1.3",
    "babel-plugin-i18next-extract": "^1.0.0",
    "eslint": "^9.26.0",
    "eslint-plugin-i18next": "^6.1.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "loki": "^0.35.1",
    "npm-force-resolutions": "^0.0.10",
    "playwright": "^1.52.0",
    "sass": "^1.86.3",
    "storybook": "^8.6.12",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2",
    "vite": "^5.4.18",
    "vite-plugin-babel": "^1.3.1",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.1.3"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    }
  }
}
